<van-tabbar active="{{ active }}" bind:change="onChange">
  <van-tabbar-item
    wx:for="{{ list }}"
    wx:key="{{ index }}"
    icon="{{ item.icon }}"
    info="{{ item.info }}"
  >
    {{ item.label }}
  </van-tabbar-item>
</van-tabbar>
